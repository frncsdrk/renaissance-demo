{
  "name": "renaissance-demo",
  "version": "0.2.0",
  "description": "renaissance demo",
  "main": "dist/app.min.js",
  "private": true,
  "scripts": {
    "browserify": "browserify -t babelify src/app.js -o dist/app.js",
    "uglify": "uglifyjs dist/app.js -o dist/app.min.js --compress --mangle --source-map",
    "build": "npm run browserify && npm run uglify",
    "dev": "live-server --ignore=dist,test",
    "test": "tape -r babel-register test/**/*.js | faucet",
    "test:nyc": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frncsdrk/renaissance-demo.git"
  },
  "keywords": [
    "renaissance",
    "demo",
    "component",
    "js"
  ],
  "author": "frncsdrk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frncsdrk/renaissance-demo/issues"
  },
  "homepage": "https://github.com/frncsdrk/renaissance-demo#readme",
  "dependencies": {
    "babel-core": "^6.26.0",
    "renaissance": "^0.5.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-minify": "^0.4.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2017": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.1.1",
    "faucet": "^0.0.1",
    "live-server": "^1.2.1",
    "nodemon": "^1.17.3",
    "nyc": "^13.3.0",
    "tape": "^4.9.0",
    "tape-run": "^4.0.0",
    "uglify-js": "^3.3.21"
  }
}
